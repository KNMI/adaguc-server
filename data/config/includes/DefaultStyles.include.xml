  <!-- Style configuration -->
  <Style name="rainbow" title="Rainbow colors" abstract="Drawing images with rainbow colors">
    <Legend fixedclasses="true" tickinterval="500" tickround="1">rainbow</Legend>
    <Min>0.0</Min>
    <Max>3500</Max>
  </Style>


  <Style name="elevation"  title="Elevation colors" abstract="Drawing images with elevation colors">
    <Legend fixedclasses="true" tickinterval="1" tickround="1">elevation</Legend>
    <Min>0.0</Min>
    <Max>10</Max>
  </Style>

  <Style name="isoluminant"  title="Isoluminant colors" abstract="Drawing images with isoluminant colors">
    <Legend fixedclasses="true" tickinterval="1" tickround="1">isoluminant</Legend>
    <Min>0.0</Min>
    <Max>10</Max>
  </Style>

   <Style name="no2" title="NO2 colors" abstract="Drawing  images with NO2 colors">
    <Legend fixedclasses="true" tickinterval="500" tickround="1">no2</Legend>
    <Min>0.0</Min>
    <Max>3500</Max>
    <RenderMethod>nearest,bilinear</RenderMethod>
  </Style>

    <!--Styles -->
  <Style name="auto" title="Auto style blue-white-red" abstract="Fast nearest neighbour interpolation using the nearest neighbour renderer">
    <Legend fixedclasses="true" tickround="1">bluewhitered</Legend>
    <Point pointstyle="point" textcolor="#111111"/>
    <Vector vectorstyle="barb" linecolor="#0000FF" />
    <Scale>0</Scale>
    <Offset>0</Offset>
    <RenderSettings interpolationmethod="bilinear"/>
    <RenderMethod>nearest</RenderMethod>
    <StandardNames standard_name="*"/>
  </Style>


  <Style name="autogeneric" title="Auto style blue-white-red" abstract="Nearest neighbour interpolation using the generic renderer">
    <Legend fixedclasses="true" tickround="1">bluewhitered</Legend>
    <Point pointstyle="point" textcolor="#111111"/>
    <Vector vectorstyle="barb" linecolor="#0000FF" />
    <Scale>0</Scale>
    <Offset>0</Offset>
    <RenderSettings interpolationmethod="nearest"/>
    <RenderMethod>generic</RenderMethod>
    <StandardNames standard_name="*"/>
  </Style>

  <Style name="autobilinear" title="Auto style bilnear blue-white-red" abstract="Bilinear neighbour interpolation using the generic renderer">
    <Legend fixedclasses="true" tickround="1">bluewhitered</Legend>
    <Point pointstyle="point" textcolor="#111111"/>
    <Vector vectorstyle="barb" linecolor="#0000FF" />
    <Scale>0</Scale>
    <Offset>0</Offset>
    <RenderSettings interpolationmethod="bilinear"/>
    <RenderMethod>generic</RenderMethod>
    <StandardNames standard_name="*"/>
  </Style>

  <Style name="autoboxoutline" title="Grid boxes" abstract="Render grid boxes if applicable">
    <Min>0</Min>
    <Max>2</Max>
    <!-- <ShadeInterval min="0.00" max="1"    label="Base grid"    fillcolor="#80808080"/> -->
    <ShadeInterval min="1" max="2"    label="Grid box borders"    fillcolor="#000000"/>
    <ShadeInterval min="2" max="3"    label="Field border left"    fillcolor="#8080FF"/>
    <ShadeInterval min="3" max="4"    label="Field border bottom"    fillcolor="#10BF10"/>
    <ShadeInterval min="4" max="5"    label="Field border right"    fillcolor="#FF0000"/>
    <ShadeInterval min="5" max="6"    label="Field border top"    fillcolor="#FFFF00"/>

    <RenderSettings interpolationmethod="nearest" drawgridboxoutline="true"/>
    <RenderMethod>generic</RenderMethod>
    <StandardNames standard_name="*"/>
  </Style>


  <Style name="autobilinear_deprecated">
    <Legend fixedclasses="true" tickround="1">bluewhitered</Legend>
    <Point pointstyle="point" textcolor="#111111"/>
    <Vector vectorstyle="barb" linecolor="#0000FF" />
    <Scale>0</Scale>
    <Offset>0</Offset>
    <RenderSettings interpolationmethod="bilinear"/>
    <RenderMethod>bilinear</RenderMethod>
    <StandardNames standard_name="*"/>
  </Style>

  <Style name="autofeatures">
    <Legend fixedclasses="true" tickround="1">elevation</Legend>
    <Point pointstyle="point" textcolor="#111111"/>
    <Vector vectorstyle="barb" linecolor="#0000FF" />
    <FeatureInterval match=".*"
        matchid="Gemeentenaam"
        bgcolor="#CCCCFF"
        fillcolor="#000080FF"
        bordercolor="#000000"
        borderwidth="4.0"
	    labelfontfile="{ADAGUC_PATH}/data/fonts/Roboto-Black.ttf"
        labelfontsize="8"
        labelcolor="#800000FF"
        labelpropertyname="Gemeentenaam"
        labelpropertyformat="%s"
        labelangle="0"
        labelpadding="3"
        /> <!-- Used in CImgRenderPolylines -->
    <Scale>0</Scale>
    <Offset>0</Offset>
    <RenderMethod>generic</RenderMethod>
    <StandardNames standard_name="features"/>
    <RenderSettings featuresoverlap="true" randomizefeatures="false"/>
  </Style>

</Configuration>
