  <!-- Style configuration -->
  <Style name="rainbow">
    <Legend fixedclasses="true" tickinterval="500" tickround="1">rainbow</Legend>
    <Min>0.0</Min>
    <Max>3500</Max>
    <NameMapping name="nearest"   title="Rainbow colors" abstract="Drawing images with rainbow colors"/>
  </Style>
  
    
  <Style name="elevation">
    <Legend fixedclasses="true" tickinterval="1" tickround="1">elevation</Legend>
    <Min>0.0</Min>
    <Max>10</Max>
    <NameMapping name="nearest"   title="Elevation colors" abstract="Drawing images with elevation colors"/>
  </Style>
  
  <Style name="isoluminant">
    <Legend fixedclasses="true" tickinterval="1" tickround="1">isoluminant</Legend>
    <Min>0.0</Min>
    <Max>10</Max>
    <NameMapping name="nearest"   title="Isoluminant colors" abstract="Drawing images with isoluminant colors"/>
  </Style>
  
   <Style name="no2">
    <Legend fixedclasses="true" tickinterval="500" tickround="1">no2</Legend>
    <Min>0.0</Min>
    <Max>3500</Max>
    <RenderMethod>nearest,bilinear</RenderMethod>
    <NameMapping name="nearest"   title="NO2 colors" abstract="Drawing  images with NO2 colors"/>
    <NameMapping name="bilinear"   title="NO2 colors smooth" abstract="Drawing images with NO2 colors and bilinear interpolation"/>
  </Style>

    <!--Styles -->
  <Style name="auto">
    <Legend>elevation</Legend>
    <Scale>0</Scale>
    <Offset>0</Offset>
    <RenderMethod>nearest,bilinear,point</RenderMethod>
      <StandardNames standard_name="*"/>
  </Style>

