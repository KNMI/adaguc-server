    <Point plotstationid="false" pointstyle="point" discradius="15" textradius="0" dot="false" fontsize="8" textcolor="#000000" />
  </Style>

  <Style name="pressure">
    <Legend fixed="true" tickinterval="4">pressure</Legend>
    <RenderMethod>point</RenderMethod>
    <Min>960</Min>
    <Max>1040</Max>
