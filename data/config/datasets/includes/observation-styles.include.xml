  <Style name="barb" title="Wind barbs with stations id" abstract="Wind barbs with stations id">
    <Legend fixed="true" tickinterval="1" textformatting="%0.1f">bluewhitered</Legend>
    <Min>0</Min>
    <Max>16</Max>
    <RenderMethod>point</RenderMethod>
    <Vector vectorstyle="barb" linewidth="2" linecolor="#0000FFFF" plotstationid="true" />
    <FilterPoints use="06235,06280,06260,06290,06310,06380"/>
  </Style>

  <Style name="barbthinned" title="Wind barbs thinned" abstract="Wind barbs thinned">
    <Legend fixed="true" tickinterval="1" textformatting="%0.1f">bluewhitered</Legend>
    <Min>0</Min>
    <Max>16</Max>
    <Thinning radius="40"/>
    <RenderMethod>point</RenderMethod>
    <Vector vectorstyle="barb" linewidth="2" linecolor="#0000FFFF" plotstationid="false" plotvalue="true" textformat="%0.0f"/>
  </Style>
  
  <Style name="barball" title="Wind barbs" abstract="Wind barbs unthinned">
